{
    "name": "inteleon/export-builder",
    "type": "library",
    "description": "Export data to pdf and csv",
    "keywords": [
        "inteleon",
        "export-builder"
    ],
    "homepage": "https://github.com/inteleon/export-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Ivarsson",
            "email": "johan@smspark.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "inteleon/extended-fpdf": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Inteleon\\ExportBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inteleon\\ExportBuilder\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
